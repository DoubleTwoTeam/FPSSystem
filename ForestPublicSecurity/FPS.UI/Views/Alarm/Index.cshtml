@model List<FPS.Models.Alarm>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/dist/layerStyle/css/default.css" type="text/css" rel="stylesheet" />
    <link href="~/dist/layerStyle/css/component.css" type="text/css" rel="stylesheet" />
    <script src="~/dist/layerStyle/js/modernizr.custom.js"></script>


    <script src="~/js/jquery-3.3.1.js"></script>
    <script src="~/js/jquery.unobtrusive-ajax.js"></script>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="~/dist/My97DatePicker/WdatePicker.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue"></script>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>天津市滨海新区公安塘沽分局矛盾纠纷系统</title>
    <link href="/police/css/style.css" rel="stylesheet" type="text/css" />



    <script type="text/javascript">
        $(document).ready(function () {
            $(".click").click(function () {
                $(".tip").fadeIn(200);
            });

            $(".tiptop a").click(function () {
                $(".tip").fadeOut(200);
            });

            $(".sure").click(function () {
                $(".tip").fadeOut(100);
            });

            $(".cancel").click(function () {
                $(".tip").fadeOut(100);
            });
        });
    </script>
</head>
<body>
    <div class="place">
        <span>位置：</span>
        <ul class="placeul">
            <li><a href="home.html">首页</a></li>
            <li><a href="#">主动排查</a></li>
            <li><a href="#">多元化排查</a></li>
            <li><a href="#">转递记录管理</a></li>
        </ul>
    </div>
    <div class="rightinfo">
        <ul class="prosearch">
            <li>
                <i>姓&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;名</i>
                <a><input name="name" type="text" class="scinput" /></a>
                <i>联系电话</i><a><input name="phone" type="text" class="scinput" /></a>
                <i>身份证号</i><a><input name="idcard" type="text" class="scinput" /></a>
            </li>
            <li>
                <a><input name="" type="button" class="sure" value="查询" style="background:#417EB5;" /></a>
                <a><input name="reset" type="reset" class="sure" value="清空" style="background:#417EB5;margin-left: 25px;" /></a>
            </li>
        </ul>
        <ul class="prosearch">
            <li>
                <i>案情地点</i><a><input name="detailSplace" type="text" class="scinput" /></a>
                <i>报警时间</i><a><input name="beginTime" type="text" class="scinput" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'})" />—<input name="endTime" type="text" class="scinput" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'})" /></a>
            </li>
        </ul>
        <table class="tablelist">
            <thead>
                <tr>
                    <td style="width:3%;">序号</td>
                    <td style="width:15%;">报警地点</td>
                    <td style="width:100px;">申报事项</td>
                    <td style="width:5%;">报警人</td>
                    <td style="width:8%;">联系方式</td>
                    <td style="width:15%;">身份证号</td>
                    <td style="width:9%;">报警时间</td>
                    <td style="width:5%;">出警人</td>
                    <td style="width:9%;">处理时间</td>
                    <td style="width:5%;">状态</td>
                    <td style="width:20px;">操作</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr class="text-center">
                        <td>@item.ID</td>
                        <td>@item.DetailSplace</td>
                        <td>@item.AlarmReason</td>
                        <td>@item.AlarmPeople</td>
                        <td>@item.Phone</td>
                        <td>@item.IdCard</td>
                        <td>@item.Time.ToString("yyyy-MM-dd hh:mm")</td>
                        <td>@item.SolvePeopleId</td>
                        <td>@item.OverTime.ToString("yyyy-MM-dd hh:mm")</td>
                        <td>@item.State</td>
                        <td>
                            <button class="md-trigger" data-modal="modal-2">从右侧滑动</button>
                            <a><input name="" type="button" class="sure" value="出警" style="background:#ff0000;" /></a>
                            <a style="display:none;"><input name="" type="button" class="sure" value="归队" style="background:#417EB5;" /></a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="pagin">
            <div class="message">共<i class="blue">1256</i>条记录，当前显示第&nbsp;<i class="blue">2&nbsp;</i>页</div>
            <ul class="paginList">
                <li class="paginItem"><a href="javascript:;"><span class="pagepre"></span></a></li>
                <li class="paginItem"><a href="javascript:;">1</a></li>
                <li class="paginItem current"><a href="police/javascript:;">2</a></li>
                <li class="paginItem"><a href="javascript:;">3</a></li>
                <li class="paginItem"><a href="javascript:;">4</a></li>
                <li class="paginItem"><a href="javascript:;">5</a></li>
                <li class="paginItem more"><a href="javascript:;">...</a></li>
                <li class="paginItem"><a href="javascript:;">10</a></li>
                <li class="paginItem"><a href="javascript:;"><span class="pagenxt"></span></a></li>
            </ul>
        </div>
    </div>

    <div class="md-modal md-effect-2" id="modal-2">
        <div class="md-content">
            <h3>模态对话框</h3>
            <div>
                <p>这是一个模式窗口。你可以做以下的事:</p>
                <ul>
                    <li><strong>读:</strong> 模态窗口可能会告诉你一些重要的事情，所以别忘了读他们说什么</li>
                    <li><strong>看:</strong> 模态窗口享受一种关注,看看它,欣赏它的存在。</li>
                    <li><strong>关闭:</strong> 点击下面的按钮关闭模态</li>
                </ul>
                <button class="md-close">关闭</button>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        $('.tablelist tbody tr:odd').addClass('odd');
    </script>
    <script src="~/dist/layerStyle/js/classie.js"></script>
    <script src="~/dist/layerStyle/js/modalEffects.js"></script>
</body>
</html>
