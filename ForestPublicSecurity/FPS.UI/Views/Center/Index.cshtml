                $(".neirong").stop().animate({ marginLeft: 290, }, 500);
                event.stopPropagation(); //阻止冒泡
                var h = $(".left-menu .nav").height();
                var h2 = $(window).height();
                var h3 = h / h2;
                if (h3 > (54 / 100)) {

                    $(".left-menu .nav > li a span").css("display", "none");
                }
                else {
                    $(".left-menu .nav > li a span").css("display", "inline-block");
                }
            }
        )

        $(".black-box").click(
            function () {
                $(".black-box").fadeOut(500);
                $(".neirong").stop().animate({ marginLeft: 0, }, 500);
                $(".left-menu").stop().animate({ marginLeft: -290, }, 500);
            }
        )
        /* --end-- */

        /* 二级菜单显示隐藏效果 */
        $(".child-nav").hide();
        $(".nav > li").mouseenter(
            function () {
                $(this).children(".child-nav").show().fadeIn(500);
            }
        )
        $(".nav > li").mouseleave(
            function () {
                $(this).children(".child-nav").hide().fadeOut(500);
            }
        )
        /* --end-- */

        /* 友情链接点击事件 */
        var z = true;
        $(".link").click(
            function () {
                if (z == true) {
                    $(this).children("ul").css("display", "block");
                    z = false;
                }
                else {
                    $(this).children("ul").css("display", "none");
                    z = true;
                }
            }
        )
/* --end-- */
    </script>
